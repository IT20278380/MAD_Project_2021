<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cus_Register">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/appname"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/green"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/ic_baseline_arrow_back_24"
        app:title="@string/C.reApp_name"
        app:titleMarginStart="40dp" />

    <TextView
        android:id="@+id/C.mainName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="100dp"
        android:text="@string/C.main_name"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/C.reName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="180dp"
        android:text="@string/C.reName"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/C.reEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="250dp"
        android:text="@string/C.reEmail"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="249dp" />

    <TextView
        android:id="@+id/C.reMobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="320dp"
        android:text="@string/C.reMobile"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="331dp" />

    <TextView
        android:id="@+id/C.rePass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="390dp"
        android:text="@string/C.rePass"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="404dp" />

    <TextView
        android:id="@+id/C.rePro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="460dp"
        android:text="@string/C.rePro"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="480dp" />

    <EditText
        android:id="@+id/C.reEnName"
        android:layout_width="300dp"
        android:layout_height="38dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="191dp"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/C.reEnEmail"
        android:layout_width="300dp"
        android:layout_height="38dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="270dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="271dp"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/C.reEnMobile"
        android:layout_width="300dp"
        android:layout_height="38dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="340dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="352dp"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/C.reEnPass"
        android:layout_width="300dp"
        android:layout_height="38dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="410dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="426dp"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/Cu_but_update"
        android:layout_width="183dp"
        android:layout_height="42dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="550dp"
        android:background="@color/green"
        android:textColor="@color/white"
        android:text="@string/C.re_but"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="600dp" />

    <TextView
        android:id="@+id/C_re_li"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="610dp"
        android:layout_marginStart="90dp"
        android:text="@string/C.re_li"
        android:textColor="#2196F3"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="676dp" />

    <Spinner
        android:id="@+id/C_re_spinner"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="480dp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="511dp" />

</RelativeLayout>