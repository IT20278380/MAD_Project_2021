<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".IT20278380.Cus_Profile">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginStart="140dp"
        android:src="@mipmap/image_foreground" />

    <TextView
        android:id="@+id/c_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_marginTop="120dp"
        android:text="Janaka Bandara"
        android:textColor="#000000"
        android:textSize="35sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="180dp"
        android:text="Email :-"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/c_Emname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="150dp"
        android:layout_marginTop="180dp"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="240dp"
        android:text="Mobile Number :-"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/c_Pmobile"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="240dp"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="300dp"
        android:text="Province :-"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/c_Ppro"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="300dp"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="360dp"
        android:text="Password :-"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/c_Ppass"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="360dp"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="211dp" />

    <Button
        android:id="@+id/c_Pup"
        android:background="@color/green"
        android:textColor="@color/white"
        android:layout_marginTop="450dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:textSize="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Update Profile" />

    <Button
        android:id="@+id/c_Pdel"
        android:background="@color/green"
        android:textColor="@color/white"
        android:layout_marginTop="520dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:textSize="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Delete Profile" />


</FrameLayout>