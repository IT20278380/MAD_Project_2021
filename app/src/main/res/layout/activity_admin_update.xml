<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin_Update">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/appname"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/green"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/ic_baseline_arrow_back_24"
        app:title="@string/D_up.appname"
        app:titleMarginStart="40dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="105dp"
        android:layout_marginTop="80dp"
        android:text="@string/D_up.appname"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/A_up.uname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="190dp"
        android:text="@string/D_up.name"
        android:textColor="#020202"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="308dp" />

    <TextView
        android:id="@+id/D_up.email0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="240dp"
        android:text="@string/D_up.email"
        android:textColor="#020202"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="387dp" />

    <TextView
        android:id="@+id/D_up_pass0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="420dp"
        android:text="Password :-"
        android:textColor="#020202"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="461dp" />

    <TextView
        android:id="@+id/D_up.num0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="330dp"
        android:text="@string/D_up.phone"
        android:textColor="#020202"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="461dp" />

    <EditText
        android:id="@+id/A_up_pass"
        android:layout_width="293dp"
        android:layout_height="41dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="450dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="411dp" />


    <TextView
        android:id="@+id/A_Up_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="190dp"
        android:textColor="#020202"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="461dp" />

    <EditText
        android:id="@+id/A_Up_email"
        android:layout_width="293dp"
        android:layout_height="41dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="270dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="484dp" />

    <EditText
        android:id="@+id/A_Up_num"
        android:layout_width="293dp"
        android:layout_height="41dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="360dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="411dp" />

    <Button
        android:id="@+id/A_up_but00"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="550dp"
        android:background="@color/green"
        android:text="@string/D_up_but"
        android:textColor="@color/white"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="584dp" />

</RelativeLayout>